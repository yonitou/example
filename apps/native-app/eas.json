{
	"cli": { "requireCommit": true, "version": "2.1.0" },
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"releaseChannel": "development",

			"env": {
				"APP_ENV": "development"
			}
		},
		"simulator": {
			"extends": "development",
			"ios": {
				"simulator": true
			}
		},
		"preview": {
			"distribution": "internal",
			"releaseChannel": "preview",
			"env": {
				"APP_ENV": "preview"
			}
		},
		"production": {
			"releaseChannel": "production",
			"env": {
				"APP_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {
			"android": {
				"serviceAccountKeyPath": "./google-service-account-api-key.json"
			},
			"ios": {
				"ascAppId": "1494676431",
				"appleTeamId": "T2JN7JTM5G"
			}
		}
	}
}
