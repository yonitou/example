{
	"cli": { "requireCommit": true },
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"releaseChannel": "development",

			"env": {
				"NX_ENV": "development"
			}
		},
		"simulator": {
			"extends": "development",
			"ios": {
				"simulator": true
			}
		},
		"staging": {
			"distribution": "internal",
			"releaseChannel": "staging",
			"env": {
				"NX_ENV": "staging"
			}
		},
		"production": {
			"releaseChannel": "production",
			"env": {
				"NX_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {
			"android": {
				"serviceAccountKeyPath": "./google-service-account-api-key.json"
			},
			"ios": {
				"ascAppId": "1494676431",
				"appleTeamId": "T2JN7JTM5G"
			}
		}
	}
}
