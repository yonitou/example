{
	"name": "native-app",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/native-app/src",
	"projectType": "application",
	"targets": {
		"start": {
			"executor": "@nrwl/expo:start",
			"options": {
				"port": 8081
			}
		},
		"serve": {
			"executor": "nx:run-commands",
			"options": {
				"command": "nx start native-app"
			}
		},
		"run-ios": {
			"executor": "@nrwl/expo:run",
			"options": {
				"platform": "ios"
			}
		},
		"run-android": {
			"executor": "@nrwl/expo:run",
			"options": {
				"platform": "android"
			}
		},
		"build": {
			"executor": "@nrwl/expo:build",
			"defaultConfiguration": "simulator",
			"options": {
				"platform": "android",
				"nonInteractive": true,
				"wait": true,
				"clearCache": true,
				"local": true
			},
			"configurations": {
				"simulator": {
					"profile": "simulator"
				},
				"development": {
					"profile": "development"
				},
				"staging": {
					"profile": "staging"
				},
				"production": {
					"profile": "production",
					"autoSubmit": true,
					"autoSubmitWithProfile": "production"
				}
			}
		},
		"build-list": {
			"executor": "@nrwl/expo:build-list",
			"options": {}
		},
		"download": {
			"executor": "@nrwl/expo:download",
			"options": {
				"output": "apps/native-app/dist"
			}
		},
		"build-ios": {
			"executor": "@nrwl/expo:build-ios",
			"options": {}
		},
		"build-android": {
			"executor": "@nrwl/expo:build-android",
			"options": {}
		},
		"build-web": {
			"executor": "@nrwl/expo:build-web",
			"options": {}
		},
		"build-status": {
			"executor": "@nrwl/expo:build-web",
			"options": {}
		},
		"sync-deps": {
			"executor": "@nrwl/expo:sync-deps",
			"options": {}
		},
		"ensure-symlink": {
			"executor": "@nrwl/expo:ensure-symlink",
			"options": {}
		},
		"publish": {
			"executor": "@nrwl/expo:publish",
			"options": {}
		},
		"publish-set": {
			"executor": "@nrwl/expo:publish-set",
			"options": {}
		},
		"rollback": {
			"executor": "@nrwl/expo:rollback",
			"options": {}
		},
		"prebuild": {
			"executor": "@nrwl/expo:prebuild",
			"options": {}
		},
		"eject": {
			"executor": "nx:run-commands",
			"options": {
				"command": "nx prebuild native-app"
			}
		},
		"install": {
			"executor": "@nrwl/expo:install",
			"options": {}
		},
		"update": {
			"executor": "@nrwl/expo:update",
			"options": {}
		},
		"export": {
			"executor": "@nrwl/expo:export",
			"options": {
				"platform": "all",
				"outputDir": "../../dist/apps/native-app"
			}
		},
		"export-web": {
			"executor": "@nrwl/expo:export",
			"options": {
				"bundler": "webpack"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/native-app/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"tsc": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					{
						"command": "tsc --noEmit -p apps/native-app/tsconfig.json"
					}
				]
			}
		}
	},

	"tags": []
}
